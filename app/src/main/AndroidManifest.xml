<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compose"
        tools:targetApi="31">
        <activity
            android:name=".TimerComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_timer_compose"
            android:theme="@style/Theme.Compose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DraggableMusicKnobActivity"
            android:exported="true"
            android:label="@string/title_activity_draggable_music_knob"
            android:theme="@style/Theme.Compose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CircularProgressBarCanvasActivity"
            android:exported="true"
            android:label="@string/title_activity_circular_progress_bar_canvas"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".ProgressBarActivity"
            android:exported="true"
            android:label="@string/title_activity_progress_bar"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".EffectHandlersActivity"
            android:exported="true"
            android:label="@string/title_activity_effect_handlers"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".EffectHandlersThemeActivity"
            android:exported="true"
            android:label="@string/title_activity_effect_handlers_theme"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".ConstraintLayoutActivity"
            android:exported="true"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".ListComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_list_compose"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".TextButtonSnackbarActivity"
            android:exported="true"
            android:label="@string/title_activity_text_button_snackbar"
            android:theme="@style/Theme.Compose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Compose" />
    </application>

</manifest>